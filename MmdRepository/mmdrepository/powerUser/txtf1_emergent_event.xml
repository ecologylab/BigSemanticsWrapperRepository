<meta_metadata_repository name="emergent_event"
	package="crit.semantics.generated.txtf1.fieldreports.emergent_event">
	
	<meta_metadata name="emergent_event" extends="metadata" comment="an emergent event" >
		<composite
			name="photo" type="image">
		</composite>
		<scalar name="initial_time" scalar_type="Date" comment="Timestamp of the beginning of the incident" />
		<composite name="event_location" type="gis_location" />
	</meta_metadata>
	
	<meta_metadata name="emergent_event_collection" extends="metadata" comment="a collection of emergent events">
		<collection name="emergent_events" child_type="emergent_event" />
		<scalar name="version" scalar_type="Float" />
		<scalar name="metadata_version" scalar_type="Float" />
	</meta_metadata>
	
</meta_metadata_repository>